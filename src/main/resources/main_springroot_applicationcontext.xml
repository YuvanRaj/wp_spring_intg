	<import resource="spring_instrumentaion_applicationcontext.xml"/>
	<import resource="spring_resource_applicationcontext.xml"/>
    <import resource="spring_orxm_applicationcontext.xml"/>
	<import resource="spring_jdbc_applicationcontext.xml"/>
	<import resource="spring_jms_applicationcontext.xml"/>
	<import resource="spring_rest_applicationcontext.xml"/>
	<import resource="spring_socket_applicationcontext.xml"/>
	<import resource="spring_quartz_applicationcontext.xml"/>
	<import resource="spring_batch_applicationcontext.xml"/>
	<import resource="spring_cache_applicationcontext.xml"/>
	<import resource="spring_ldap_applicationcontext.xml"/>
</beans>