<sql-dataset>
	<sql-info query-name="selectUserDetailsById">
		<sql-query>
			SELECT USER_NAME,EMAIL FROM USERS WHERE USER_ID=?
		</sql-query>
		<sql-set-properties>
			<property name="userName" type="CDT_STRING" />
			<property name="email" type="CDT_STRING" />
		</sql-set-properties>
		<sql-get-properties>
			<property name="userId" type="CDT_INTGER" />
		</sql-get-properties>
	</sql-info>
</sql-dataset>